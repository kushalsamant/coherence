{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "redirects": [
    {
      "source": "/about.html",
      "destination": "/history",
      "permanent": true
    },
    {
      "source": "/people.html",
      "destination": "/history#people",
      "permanent": true
    },
    {
      "source": "/projects.html",
      "destination": "/history#projects",
      "permanent": true
    },
    {
      "source": "/anthology.html",
      "destination": "/history#anthology",
      "permanent": true
    },
    {
      "source": "/support.html",
      "destination": "/getintouch",
      "permanent": true
    },
    {
      "source": "/contact.html",
      "destination": "/getintouch",
      "permanent": true
    },
    {
      "source": "/projects/:slug.html",
      "destination": "/projects/:slug",
      "permanent": true
    },
    {
      "source": "/anthology/:slug.html",
      "destination": "/anthology/:slug",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "ask.kvshvl.in"
        }
      ],
      "destination": "/ask/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.ask.kvshvl.in"
        }
      ],
      "destination": "/ask/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "reframe.kvshvl.in"
        }
      ],
      "destination": "/reframe/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.reframe.kvshvl.in"
        }
      ],
      "destination": "/reframe/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "sketch2bim.kvshvl.in"
        }
      ],
      "destination": "/sketch2bim/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.sketch2bim.kvshvl.in"
        }
      ],
      "destination": "/sketch2bim/:path*"
    }
  ]
}

