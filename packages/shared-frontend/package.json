{
  "name": "@kvshvl/shared-frontend",
  "version": "1.0.0",
  "description": "Shared frontend utilities for KVSHVL platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./auth": {
      "types": "./dist/auth/index.d.ts",
      "default": "./dist/auth/index.js"
    },
    "./payments": {
      "types": "./dist/payments/index.d.ts",
      "default": "./dist/payments/index.js"
    },
    "./cost-monitoring": {
      "types": "./dist/cost-monitoring/index.d.ts",
      "default": "./dist/cost-monitoring/index.js"
    },
    "./settings": {
      "types": "./dist/settings/index.d.ts",
      "default": "./dist/settings/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "kvshvl",
    "shared",
    "frontend",
    "typescript"
  ],
  "author": "Kushal Samant",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "next": "^14.0.0 || ^15.0.0 || ^16.0.0",
    "razorpay": "^2.9.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.7.0"
  }
}
